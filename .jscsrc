{
  "preset": "google",
  "fileExtensions": [
    ".js"
  ],
  "validateIndentation": {
    "value": 4,
    "allExcept": [
      "comments"
    ]
  },
  "maximumLineLength": 120,
  "validateLineBreaks": "LF",
  "maxErrors": 5000,
  "safeContextKeyword": [
    "_this",
    "self"
  ],
  "excludeFiles": [
    "test/**",
    "node_modules/**",
    "dist/**",
    "js/editors/bower_components/**",
    "js/editors/node_modules/**",
    "js/sbg/angular/**",
    "js/sbg/angular/*.js",
    "js/sbg/angular-mock/**",
    "js/sbg/angular-mock/*.js",
    "js/editors/**/template.js"
  ]
}